@model IEnumerable<RoslynMapper.Benchmark.BenchmarkResult>
@{
    Layout = null;
}
<div class="row">
    <div class="col-lg-12">
        <table class="table">
            <thead>
                <tr>
                    <td>Name</td>
                    <td>Count</td>
                    <td>Initialize</td>
                    <td>Elapse</td>
                </tr>
            </thead>
            <tbody>
                @foreach (var result in Model)
                {
                    <tr>
                        <td>@result.MapperName</td>
                        <td>@result.Count</td>
                        <td>@result.Initialize</td>
                        <td>@result.Elapse</td>
                    </tr>
                }
            </tbody>

        </table>
    </div>
</div>


